<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0 http://www.kde.org/standards/kcfg/1.0/kcfg.xsd">
    <kcfgfile name=""/>
    <group name="Preferences">
        <entry name="refreshInterval" type="Int">
            <default>8</default>
        </entry>
        <entry name="full" type="Enum">
            <label>0 = 便笺, 1 = 胡桃</label>
            <default>0</default>
        </entry>

    </group>
    <group name="Configuration">
        <entry name="uid" type="String">
            <default>100461712</default>
        </entry>
        <entry name="server" type="Enum">
            <label>0 = cn_gf01 官服, 1 = cn_qd01 B服</label>
            <default>0</default>
        </entry>

        <entry name="cookie" type="String">
            <default>Hello World</default>
        </entry>
    </group>
    <group name="Config">
        <entry name="proxyEnable" type="Enum">
            <label>0 = 不使用代理, 1 = 系统代理, 2 = 手动代理</label>
            <default>0</default>
        </entry>
        <entry name = "proxyType" type="Enum">
            <label>0 = http, 1 = socks5</label>
            <default>0</default>
        <entry name="proxyHost" type="String">
            <default>127.0.0.1</default>
        </entry>
        <entry name="proxyPort" type="Int">
            <default>8080</default>
        </entry>
        <entry name="proxyUsername" type="String">
            <default></default>
        </entry> 
        <entry name="proxyPassword" type="String">
            <default></default>
        </entry> 
    </group>
</kcfg>